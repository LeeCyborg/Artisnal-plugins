{
"manifest_version": 2,
    "name": "Artisanal Plug-In",
    "description": "dooper",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Do It To It",
      "default_icon": "icon.png"
  }
}