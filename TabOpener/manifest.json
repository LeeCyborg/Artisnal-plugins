{
"manifest_version": 2,
    "name": "Artisanal Plug-In",
    "description": "booper",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
},
  "browser_action": {
      "default_title": "Do It To It",
      "default_icon": "icon.png"
  }
}

